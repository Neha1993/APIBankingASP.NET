@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model APIBankingASP.NET.Models.FundsTransferByCustomerService2.GetBalanceResult
@using (Html.BeginForm(new { @class = "form-horizontal" }))
{
    <fieldset>
        <legend>Result</legend>
        <div class="form-group">
            @Html.LabelFor(m => m.Version, new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.Version, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.accountCurrencyCode, new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.accountCurrencyCode, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.accountBalanceAmount, new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.accountBalanceAmount, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.lowBalanceAlert, new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.lowBalanceAlert, new { @class = "form-control" })
        </div>
    </fieldset>

}
