@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@model APIBankingASP.NET.Models.FundsTransferByCustomerService2.GetBalanceRequest
@using (Html.BeginForm(new { @class = "form-horizontal" }))
{
    @Html.ValidationSummary(true)

    @Html.EditorFor(m => m);

    <fieldset>
        <legend>Request</legend>
        <div class="form-group">
            @Html.LabelFor(m => m.appID, new { @class = "control-label col-md-3" })
            @Html.EditorFor(m => m.appID, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.customerID, new { @class = "control-label col-md-3" })
            @Html.EditorFor(m => m.customerID, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.AccountNumber, new { @class = "control-label col-md-3" })
            @Html.EditorFor(m => m.AccountNumber, new { @class = "form-control" })
        </div>
    </fieldset>
    <button type="submit">Call</button>
}
