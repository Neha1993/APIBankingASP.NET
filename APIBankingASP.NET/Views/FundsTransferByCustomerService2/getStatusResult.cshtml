@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model APIBankingASP.NET.Models.FundsTransferByCustomerService2.GetStatusResult
@using (Html.BeginForm(new { @class = "form-horizontal" }))
{
    <fieldset>
        <legend>Result</legend>
        <div class="form-group">
            @Html.LabelFor(m => m.version, new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.version, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.transferType, new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.transferType, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.reqTransferType, new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.reqTransferType, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.transactionDate, new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.transactionDate, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.transferAmount, new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.transferAmount, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.transferCurrencyCode, new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.transferCurrencyCode, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.Label("statusCode", new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.transferStatus.statusCode, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.Label("bankReferenceNo", new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.transferStatus.bankReferenceNo, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.Label("reason", new { @class = "control-label col-md-3" })
            @Html.DisplayFor(m => m.transferStatus.reason, new { @class = "form-control" })
        </div>
    </fieldset>

}
